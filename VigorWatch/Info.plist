<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundleDisplayName</key>
    <string>Vigor Bridge</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>1.1</string>
    <key>CFBundleVersion</key>
    <string>2</string>
    <key>CFBundleIconName</key>
    <string>AppIcon</string>
    <key>NSHealthShareUsageDescription</key>
    <string>Vigor Bridge needs access to your health data to display your heart rate, steps, and floors climbed.</string>
    <key>NSHealthUpdateUsageDescription</key>
    <string>Vigor Bridge may save your health metrics to Apple Health.</string>
    <key>WKApplication</key>
    <true/>
    <key>WKWatchOnly</key>
    <false/>
    <key>WKCompanionAppBundleIdentifier</key>
    <string>cloud.buggygames.vigor</string>
    <key>WKBackgroundModes</key>
    <array>
        <string>self-care</string>
    </array>
    <key>BGTaskSchedulerPermittedIdentifiers</key>
    <array>
        <string>cloud.buggygames.vigor.watch.refresh</string>
    </array>
</dict>
</plist>
